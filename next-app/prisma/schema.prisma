generator client {
  provider = "prisma-client-js"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user {
  id    String @id @default(uuid())
  email String @unique
  name  String 
  image String
  role  Role @default(USER)
}

enum Role {
  USER
  ADMIN
}